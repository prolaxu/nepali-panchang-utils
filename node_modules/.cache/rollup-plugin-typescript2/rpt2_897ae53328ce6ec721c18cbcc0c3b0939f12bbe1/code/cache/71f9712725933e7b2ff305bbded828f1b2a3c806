{"code":"import { MhahPanchangImpl } from './mhahPanchangImpl';\r\nexport class MhahCalculateFunc {\r\n    calculate(d, mhahConstant) {\r\n        let mhahPanchangImpl = new MhahPanchangImpl(mhahConstant);\r\n        let Day = {};\r\n        let Tithi = {};\r\n        let Paksha = {};\r\n        let Nakshatra = {};\r\n        let Karna = {};\r\n        let Yoga = {};\r\n        let Ayanamsa = {};\r\n        let Raasi = {};\r\n        let Julian = {};\r\n        var n_wday, n_tithi = 1, n_naksh = 1, n_karana = 0, n_yoga;\r\n        //var mhahPanchangImpl = new MhahPanchangImpl();\r\n        var day = d.getDate();\r\n        var mon = d.getMonth() + 1;\r\n        var year = d.getFullYear();\r\n        var hr = d.getHours();\r\n        hr += d.getMinutes() / 60;\r\n        var tzone = (d.getTimezoneOffset() / 60) * -1;\r\n        let inpmin = Math.floor(d.getMinutes());\r\n        if (inpmin < 10)\r\n            inpmin = '0' + inpmin;\r\n        // Julian date in local p. LT:\r\n        let dayhr = day + hr / 24;\r\n        let jdlt = mhahPanchangImpl.mdy2julian(mon, dayhr, year);\r\n        // Day:\r\n        n_wday = mhahPanchangImpl.weekDay(jdlt);\r\n        Day.ino = n_wday;\r\n        Day.name = mhahConstant.Day.name[n_wday];\r\n        Day.name_en_UK = mhahConstant.Day.name_en_UK[n_wday];\r\n        // julian day at the begining of the day\r\n        let jd0 = mhahPanchangImpl.mdy2julian(mon, day, year);\r\n        let jdut = jd0 + (hr - tzone) / 24;\r\n        mhahPanchangImpl.dt = mhahPanchangImpl.dTime(jdut);\r\n        let jd = jdut + mhahPanchangImpl.dt / 24;\r\n        //ayyanamsa\r\n        mhahPanchangImpl.ayanamsa = mhahPanchangImpl.calcayan(jd);\r\n        // Logitudinal Moon\r\n        mhahPanchangImpl.Lmoon = mhahPanchangImpl.moon(jd);\r\n        // Logitudinal Sun\r\n        mhahPanchangImpl.Lsun = mhahPanchangImpl.sun(jd);\r\n        // yoga:\r\n        let zyoga = this.getZYoga(mhahPanchangImpl, mhahPanchangImpl.ayanamsa, jd);\r\n        n_yoga = this.getYoga(zyoga);\r\n        let s_yoga = mhahPanchangImpl.yoga(jd, zyoga, tzone);\r\n        // Nakstra\r\n        n_naksh = this.getNakshatra(mhahPanchangImpl, mhahPanchangImpl.Lmoon, mhahPanchangImpl.ayanamsa);\r\n        var s_naksh = mhahPanchangImpl.nakshatra(jd, n_naksh, tzone);\r\n        // tithi\r\n        n_tithi = this.getTithi(mhahPanchangImpl.Lmoon, mhahPanchangImpl.Lsun);\r\n        var s_tithi = mhahPanchangImpl.tithi(jd, n_tithi, tzone, 12);\r\n        // paksha\r\n        let n_paksha = this.getPaksha(n_tithi + 1);\r\n        // Karana\r\n        let KaranaArray = this.getKarana(mhahPanchangImpl.Lmoon, mhahPanchangImpl.Lsun);\r\n        n_karana = KaranaArray[0];\r\n        let nk = KaranaArray[1];\r\n        let s_karana = mhahPanchangImpl.tithi(jd, nk, tzone, 6);\r\n        let z = this.getRaasi(mhahPanchangImpl, mhahPanchangImpl.Lmoon, mhahPanchangImpl.ayanamsa);\r\n        Ayanamsa.name = mhahPanchangImpl.lon2dms(mhahPanchangImpl.ayanamsa);\r\n        Raasi.name = mhahConstant.Raasi.name[z];\r\n        Raasi.ino = z;\r\n        Raasi.name_en_UK = mhahConstant.Raasi.name_en_UK[z];\r\n        Nakshatra.name = mhahConstant.Nakshatra.name[n_naksh];\r\n        Nakshatra.name_en_IN = mhahConstant.Nakshatra.name_en_IN[n_naksh];\r\n        Nakshatra.ino = n_naksh;\r\n        Nakshatra.start = s_naksh.start;\r\n        Nakshatra.end = s_naksh.end;\r\n        Karna.name = mhahConstant.Karna.name[n_karana];\r\n        Karna.name_en_IN = mhahConstant.Karna.name_en_IN[n_karana];\r\n        Karna.ino = n_karana;\r\n        Karna.start = s_karana.start;\r\n        Karna.end = s_karana.end;\r\n        Yoga.name = mhahConstant.Yoga.name[n_yoga];\r\n        Yoga.name_en_IN = mhahConstant.Yoga.name_en_IN[n_yoga];\r\n        Yoga.ino = n_yoga;\r\n        Yoga.start = s_yoga.start;\r\n        Yoga.end = s_yoga.end;\r\n        Tithi.name = mhahConstant.Tithi.name[n_tithi];\r\n        Tithi.name_en_IN = mhahConstant.Tithi.name_en_IN[n_tithi];\r\n        Tithi.ino = n_tithi;\r\n        Tithi.start = s_tithi.start;\r\n        Tithi.end = s_tithi.end;\r\n        Paksha.ino = n_paksha;\r\n        Paksha.name = mhahConstant.Paksha.name[n_paksha];\r\n        Paksha.name_en_IN = mhahConstant.Paksha.name_en_IN[n_paksha];\r\n        Paksha.name_en_UK = mhahConstant.Paksha.name_en_UK[n_paksha];\r\n        Julian.date = jd;\r\n        Julian.day = Math.floor(jd);\r\n        return {\r\n            Day,\r\n            Tithi,\r\n            Paksha,\r\n            Nakshatra,\r\n            Karna,\r\n            Yoga,\r\n            Ayanamsa,\r\n            Raasi,\r\n            Julian,\r\n        };\r\n    }\r\n    getTithi(Lmoon, Lsun) {\r\n        if (Lmoon < Lsun)\r\n            Lmoon += 360;\r\n        return Math.floor((Lmoon - Lsun) / 12);\r\n    }\r\n    getNakshatra(mhahPanchangImpl, Lmoon, ayanamsa) {\r\n        let Lmoon0 = mhahPanchangImpl.fix360(Lmoon + ayanamsa);\r\n        return Math.floor((Lmoon0 * 6) / 80);\r\n    }\r\n    getZYoga(mhahPanchangImpl, ayanamsa, jd) {\r\n        mhahPanchangImpl.moon(jd);\r\n        mhahPanchangImpl.sun(jd);\r\n        let dmoonYoga = mhahPanchangImpl.LmoonYoga + ayanamsa - 491143.07698973856;\r\n        let dsunYoga = mhahPanchangImpl.LsunYoga + ayanamsa - 36976.91240579201;\r\n        return dmoonYoga + dsunYoga;\r\n    }\r\n    getYoga(zyoga) {\r\n        let n_yoga = (zyoga * 6) / 80;\r\n        while (n_yoga < 0)\r\n            n_yoga += 27;\r\n        while (n_yoga > 27)\r\n            n_yoga -= 27;\r\n        return Math.floor(n_yoga);\r\n    }\r\n    getKarana(Lmoon0, Lsun0) {\r\n        let n_karana = 0, nk = 0;\r\n        if (Lmoon0 < Lsun0)\r\n            Lmoon0 += 360;\r\n        nk = Math.floor((Lmoon0 - Lsun0) / 6);\r\n        if (nk === 0)\r\n            n_karana = 10;\r\n        if (nk >= 57)\r\n            n_karana = nk - 50;\r\n        if (nk > 0 && nk < 57)\r\n            n_karana = nk - 1 - Math.floor((nk - 1) / 7) * 7;\r\n        return [n_karana, nk];\r\n    }\r\n    getRaasi(mhahPanchangImpl, Lmoon, ayanamsa) {\r\n        return Math.floor(Math.abs(mhahPanchangImpl.fix360(Lmoon + ayanamsa)) / 30);\r\n    }\r\n    getPaksha(n_tithi) {\r\n        return n_tithi > 15 ? 1 : 0;\r\n    }\r\n}\r\n//# sourceMappingURL=mhahCalculateFunc.js.map","references":["/home/laxu/Desktop/astro/mhah-panchang/src/mhahConstant.ts","/home/laxu/Desktop/astro/mhah-panchang/src/mhahPanchangImpl.ts"],"map":"{\"version\":3,\"file\":\"mhahCalculateFunc.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../src/mhahCalculateFunc.ts\"],\"names\":[],\"mappings\":\"AACA,OAAO,EAAE,gBAAgB,EAAE,MAAM,oBAAoB,CAAC;AAEtD,MAAM,OAAO,iBAAiB;IAC5B,SAAS,CAAC,CAAO,EAAE,YAA0B;QAC3C,IAAI,gBAAgB,GAAG,IAAI,gBAAgB,CAAC,YAAY,CAAC,CAAC;QAC1D,IAAI,GAAG,GAAQ,EAAQ,CAAC;QACxB,IAAI,KAAK,GAAQ,EAAQ,CAAC;QAC1B,IAAI,MAAM,GAAQ,EAAQ,CAAC;QAC3B,IAAI,SAAS,GAAQ,EAAQ,CAAC;QAC9B,IAAI,KAAK,GAAQ,EAAQ,CAAC;QAC1B,IAAI,IAAI,GAAQ,EAAQ,CAAC;QACzB,IAAI,QAAQ,GAAQ,EAAQ,CAAC;QAC7B,IAAI,KAAK,GAAQ,EAAQ,CAAC;QAC1B,IAAI,MAAM,GAAQ,EAAQ,CAAC;QAE3B,IAAI,MAAM,EACR,OAAO,GAAG,CAAC,EACX,OAAO,GAAG,CAAC,EACX,QAAQ,GAAG,CAAC,EACZ,MAAM,CAAC;QACT,gDAAgD;QAEhD,IAAI,GAAG,GAAG,CAAC,CAAC,OAAO,EAAE,CAAC;QACtB,IAAI,GAAG,GAAG,CAAC,CAAC,QAAQ,EAAE,GAAG,CAAC,CAAC;QAC3B,IAAI,IAAI,GAAG,CAAC,CAAC,WAAW,EAAE,CAAC;QAC3B,IAAI,EAAE,GAAG,CAAC,CAAC,QAAQ,EAAE,CAAC;QACtB,EAAE,IAAI,CAAC,CAAC,UAAU,EAAE,GAAG,EAAE,CAAC;QAC1B,IAAI,KAAK,GAAG,CAAC,CAAC,CAAC,iBAAiB,EAAE,GAAG,EAAE,CAAC,GAAG,CAAC,CAAC,CAAC;QAE9C,IAAI,MAAM,GAAQ,IAAI,CAAC,KAAK,CAAC,CAAC,CAAC,UAAU,EAAE,CAAC,CAAC;QAC7C,IAAI,MAAM,GAAG,EAAE;YAAE,MAAM,GAAG,GAAG,GAAG,MAAM,CAAC;QAEvC,8BAA8B;QAC9B,IAAI,KAAK,GAAG,GAAG,GAAG,EAAE,GAAG,EAAE,CAAC;QAC1B,IAAI,IAAI,GAAG,gBAAgB,CAAC,UAAU,CAAC,GAAG,EAAE,KAAK,EAAE,IAAI,CAAC,CAAC;QAEzD,OAAO;QACP,MAAM,GAAG,gBAAgB,CAAC,OAAO,CAAC,IAAI,CAAC,CAAC;QACxC,GAAG,CAAC,GAAG,GAAG,MAAM,CAAC;QACjB,GAAG,CAAC,IAAI,GAAG,YAAY,CAAC,GAAG,CAAC,IAAI,CAAC,MAAM,CAAC,CAAC;QACzC,GAAG,CAAC,UAAU,GAAG,YAAY,CAAC,GAAG,CAAC,UAAU,CAAC,MAAM,CAAC,CAAC;QAErD,wCAAwC;QACxC,IAAI,GAAG,GAAG,gBAAgB,CAAC,UAA